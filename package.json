{
  "name": "tauri-starter",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "tauri-starter",
  "author": "Eiog",
  "license": "MIT",
  "homepage": "https://github.com/Eiog/tauri-starter",
  "scripts": {
    "dev": "vite",
    "tauri:dev": "tauri dev",
    "build": "vue-tsc --noEmit && vite build",
    "dev:server": "tsx watch server/index.ts",
    "build:server": "tsup",
    "start:server": "node dist-server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update:deps": "taze -w && pnpm i",
    "type:check": "vue-tsc --noEmit",
    "start": "node ./app.js",
    "preview": "vite preview",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.1",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-http": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@tauri-apps/plugin-store": "^2.0.0",
    "@tauri-apps/plugin-websocket": "^2.0.0",
    "@unhead/vue": "^1.11.7",
    "@unocss/reset": "^0.63.3",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "colord": "^2.9.3",
    "file-saver": "^2.0.5",
    "h3": "^1.13.0",
    "mixte": "^2.4.0",
    "nanoid": "^5.0.7",
    "notivue": "^2.4.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "vue": "^3.5.11",
    "vue-hooks-plus": "^2.2.1",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@eiog/ts-config": "^0.0.3",
    "@iconify-json/mage": "^1.2.0",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@tauri-apps/cli": "^2.0.1",
    "@types/node": "^22.7.4",
    "@types/nprogress": "^0.2.3",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@unocss/eslint-config": "0.63.3",
    "@unocss/eslint-plugin": "^0.63.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/ui": "^2.1.2",
    "@vue-hooks-plus/resolvers": "^1.2.5",
    "@vue/test-utils": "^2.4.6",
    "commitlint": "^19.5.0",
    "cz-git": "^1.10.0",
    "czg": "^1.10.0",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "jsdom": "^25.0.1",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "naive-ui": "^2.40.1",
    "postcss": "^8.4.47",
    "postcss-preset-env": "^10.0.6",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.17.2",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unocss": "^0.63.3",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-extra": "^0.5.3",
    "unocss-preset-scrollbar": "^0.3.1",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-info": "^1.2.1",
    "unplugin-turbo-console": "^1.10.4",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vfonts": "^0.0.3",
    "vite": "^5.4.8",
    "vite-bundle-analyzer": "^0.12.1",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-url-copy": "^1.1.4",
    "vite-plugin-version-mark": "^0.1.0",
    "vite-plugin-vue-devtools": "7.4.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-webfont-dl": "^3.9.5",
    "vitest": "^2.1.2",
    "vue-router-better-scroller": "^0.0.0",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
