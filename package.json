    "@tauri-apps/plugin-fs": "^2.0.3",
    "@tauri-apps/plugin-http": "^2.0.1",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-os": "^2.0.0",
    "@tauri-apps/plugin-positioner": "^2.1.0",
    "@tauri-apps/plugin-process": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.1",
    "@tauri-apps/plugin-store": "^2.1.0",
    "@tauri-apps/plugin-updater": "^2.0.0",
    "@tauri-apps/plugin-upload": "^2.2.1",
    "@tauri-apps/plugin-websocket": "^2.0.0",
    "@unhead/vue": "^1.11.13",
    "@unocss/reset": "^0.65.1",
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.9",
    "colord": "^2.9.3",
    "file-saver": "^2.0.5",
    "h3": "^1.13.0",
    "mixte": "3.0.0-beta.3",
    "nanoid": "^5.0.9",
    "notivue": "^2.4.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.1.3",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-hooks-plus": "^2.2.1",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@eiog/ts-config": "^0.0.3",
    "@iconify-json/mage": "^1.2.1",
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "@tauri-apps/cli": "^2.1.0",
    "@types/node": "^22.10.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@unocss/eslint-config": "0.65.1",
    "@unocss/eslint-plugin": "^0.65.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/ui": "^2.1.8",
    "@vue-hooks-plus/resolvers": "^1.2.5",
    "@vue/test-utils": "^2.4.6",
    "commitlint": "^19.6.0",
    "cz-git": "^1.11.0",
    "czg": "^1.11.0",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "jsdom": "^25.0.1",
    "less": "^4.2.1",
    "lint-staged": "^15.2.10",
    "naive-ui": "^2.40.3",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^10.1.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.18.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "unocss": "^0.65.1",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-extra": "^0.5.3",
    "unocss-preset-scrollbar": "^0.3.1",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-icons": "^0.21.0",
    "unplugin-info": "^1.2.1",
    "unplugin-turbo-console": "^1.10.6",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-router": "^0.10.9",
    "vfonts": "^0.0.3",
    "vite": "^6.0.2",
